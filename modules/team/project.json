{
  "name": "team",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/team/src",
  "prefix": "phalco",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "modules/team/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["modules/team/**/*.ts", "modules/team/**/*.html"]
      }
    }
  }
}
